        //function for setup 
        function editSessionMasterID(_refMasterID) {

            //
            debugger;
            if (_refMasterID != undefined && _refMasterID != '') {
                $.ajax({
                    type: 'post',
                    dataType: 'json',
                    cache: false,
                    url: '@Url.Action("editSession", "home")',
                    data: { inRefMasterID: _refMasterID },
                    //beforeSend: function () {
                    //    $("#ajaxLoader").show();
                    //},
                    success: function (response, textStatus, jqXHR) {
                        debugger;
                        if (response.Saved == "Yes") {
                            window.location.href = '@Url.Action("index", "setup")';
                        }
                        //$("#ajaxLoader").hide();
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                        toastr["error"](errorThrown);
                        //$("#ajaxLoader").hide();
                    }
                });
            }
            else
                displayError('No such record found!');

            return true;
        }

        //function for DrCrNote
        function ManageDrCrNote(_refType) {

            debugger;

            if (_refType != undefined && _refType != '') {
                $.ajax({
                    type: 'post',
                    dataType: 'json',
                    cache: false,
                    url: '@Url.Action("editSession", "DrCrNote")',
                    data: { pType: _refType },
                    //beforeSend: function () {
                    //    $("#ajaxLoader").show();
                    //},
                    success: function (response, textStatus, jqXHR) {
                        debugger;
                        if (response._Result == "true") {
                            window.location.href = '@Url.Action("Manage", "DrCrNote")';
                        }
                        if (response._Result == "false") {
                            toastr["warning"](response._Message);
                        }
                        //$("#ajaxLoader").hide();
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                        displayError(errorThrown);
                        //$("#ajaxLoader").hide();
                    }
                });
            }
            else
                displayError('No such record found!');

            return true;
        }

