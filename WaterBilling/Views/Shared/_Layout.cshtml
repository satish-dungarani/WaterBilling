        function LiActive() {
            // To mark active link on selected menu

            $(".sidebar-menu").on('click', 'li', function () {
                $(".sidebar-menu li.active").removeClass('active');
                $(this).addClass('active');
            });
        }

        function GetConsumer() {
            var _Search = $("#txtsearch").val()
            if (_Search == "" || _Search == undefined) {
                toastr["warning"]('Please enter any Consumer Numer in box!')
                $("#txtsearch").focus();
            }

            debugger;
            $.ajax({
                type: 'get',
                datatype: 'html',
                url: '@Url.Action("ConsumerInformationPartial", "Consumer")',
                data: { ConsumerNo: _Search },
                //beforesend: function () {
                //    debugger;
                //    $("#ajaxloader").show();
                //},
                success: function (response) {
                    debugger;
                    //$("#ajaxloader").hide();
                    $("#Mainbody").html(response);

                },
                error: function (jxex) {
                    debugger;
                    //$("#ajaxloader").hide();
                    toastr["error"](jxex);
                }
            });
        }
